<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <!-- Border -->
    <item>
        <shape>
            <solid android:color="@color/streamlist_divider"/>
        </shape>
    </item>
    <!-- Body -->
    <item
        android:bottom="0dp"
        android:left="3dp"
        android:right="3dp"
        android:top="0dp">
        <shape android:shape="rectangle">
            <solid android:width="20dp" android:color="#01395E" />
            <stroke android:width="10dp" android:color="#021C23"  />
            <size android:height="20dp"
                android:width="20dp"/>

        </shape>
    </item>
</layer-list>